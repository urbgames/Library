package objects;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;;

@Entity
public class Tag {
	
	@Id
	@GeneratedValue
	private int id;
	private String name;
	public String getNome() {
		return name;
	}
	public void setNome(String name) {
		this.name = name;
	}

	
}
